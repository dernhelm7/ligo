let case1 = (n : int) : string => {
    let output = "";
    switch (n) {
        case 1:
            output = output + "Hello";
        case 2:
            output = output + " World";
        case 3:
            output = output + " @@@";
        default:
            output = output + " !!!";
    };
    output = output + " end";
    return output;
};
let _test1 = () : unit => {
    let _1 = case1(1);
    let _2 = case1(2);
    let _3 = case1(3);
    let _4 = case1(4);
    assert (_1 == "Hello World @@@ !!! end");
    assert (_2 == " World @@@ !!! end");
    assert (_3 == " @@@ !!! end");
    assert (_4 == " !!! end");
};
let test1 = _test1();

