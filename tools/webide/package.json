{
  "name": "ligo-editor",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/client",
    "packages/server"
  ],
  "description": "",
  "scripts": {
    "start": "",
    "build-docker": "docker build ../../ -f Dockerfile --build-arg EXAMPLES_DIR_SRC=src/test/examples -t ligo-webide",
