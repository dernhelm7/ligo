  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com:ligolang/ligo-web-ide.git"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.0.0",
    "@parcel/transformer-webmanifest": "^2.0.0"
  }
}
