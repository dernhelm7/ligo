entWrapper height={topPaneHeight} className=\"col-sm-12 col-md-7 order-md-2\"><EditorComponent editorHeight={topPaneHeight} /></EditorComponentWrapper>\n            <OutputTab />\n          </SplitPane>\n          <div className=\"col-sm-12 col-md-3 order-md-3\"><TabsPanelComponent /></div>\n        </Container>\n        <FeedbackContainer>\n          <FloatButtonComponent\n            tooltip=\"Report an issue\"\n            text=\"!\"\n            href=\"https://gitlab.com/ligolang/ligo/-/issues/new?issue%5Bassignee_id%5D=&issue%5Bmilestone_id%5D=\"\n          ></FloatButtonComponent>\n          <FloatButtonComponent\n            tooltip=\"Ask a question\"\n            text=\"?\"\n            href=\"https://t.me/LigoLang\"\n          ></FloatButtonComponent>\n        </FeedbackContainer>\n        <TooltipContainer />\n      </Wrapper>\n      <Switch>\n      <Route path=\"/p/:id\">\n        <ViewSharedFile />\n      </Route>\n      <Route path=\"/\">\n        <ViewExampleFile />\n      </Route>\n      </Switch>\n      </Router>\n    </Provider>\n    </div>\n  );\n};\n\nexport default App;\n","/Users/apoorva/Documents/ecad/ligo/tools/webide/packages/client/src/configure-store.ts",["283"],"import { applyMiddleware, createStore, Middleware } from 'redux';\nimport ReduxThunk from 'redux-thunk';\n\nimport rootReducer, { AppState } from './redux/app';\n\ndeclare var defaultServerState: AppState | undefined;\n \n\nexport default function configureStore() {\n  const store = createStore(\n    rootReducer,\n    applyMiddleware(ReduxThunk, cleanRouteOnAction)\n  );\n\n  return store;\n}\n\nconst cleanRouteOnAction: Middleware = store => next => action => {\n  const { share } = store.getState();\n  next(action);\n  const state = store.getState();\n  if (\n    share && share.link !== undefined &&\n    state.share.link === undefined &&\n    window.location.pathname !== '/'\n  ) {\n    window.history.replaceState({}, document.title, '/');\n  }\n};\n","/Users/apoorva/Documents/eca